<?xml version="1.0"?>
<Container version="2">
  <Name>Krusader</Name>
  <Repository>sparklyballs/krusader</Repository>
  <Registry>https://registry.hub.docker.com/u/sparklyballs/krusader/</Registry>
  <Network>bridge</Network>
  <Privileged>false</Privileged>
  <Support>http://lime-technology.com/forum/index.php?topic=37209</Support>
  <Overview>Krusader is a fully featured file manager, highly configurable. [br][br][b][span style="color: red"]BE VERY CAREFUL AS YOU CAN DELETE THINGS YOU MAY NOT INTEND TO[br]I AM NOT RESPONSIBLE FOR YOUR BAD DECISIONS.[/span][br]Map /UNRAID to a local folder tree you wish to perform actions on. Add other mappings manually if you wish.&#xD;
  </Overview>
  <Category>Tools:</Category>
  <WebUI>http://[IP]:[PORT:8080]/#/client/c/Krusader</WebUI>
  <TemplateURL/>
  <Icon>http://i.imgur.com/uf0NDSm.png</Icon>
  <ExtraParams/>
  <DateInstalled>1499185201</DateInstalled>
  <Description>Krusader is a fully featured file manager, highly configurable. [br][br][b][span style="color: red"]BE VERY CAREFUL AS YOU CAN DELETE THINGS YOU MAY NOT INTEND TO[br]I AM NOT RESPONSIBLE FOR YOUR BAD DECISIONS.[/span][br]Map /UNRAID to a local folder tree you wish to perform actions on. Add other mappings manually if you wish.&#xD;
  </Description>
  <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort>8080</HostPort>
        <ContainerPort>8080</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <Data>
    <Volume>
      <HostDir>/mnt/</HostDir>
      <ContainerDir>/UNRAID</ContainerDir>
      <Mode>rw,slave</Mode>
    </Volume>
    <Volume>
      <HostDir>/mnt/cache/Docker/krusader</HostDir>
      <ContainerDir>/nobody/.kde</ContainerDir>
      <Mode>rw,slave</Mode>
    </Volume>
    <Volume>
      <HostDir>/mnt/disks/</HostDir>
      <ContainerDir>/UNASSIGNED</ContainerDir>
      <Mode>rw,slave</Mode>
    </Volume>
    <Volume>
      <HostDir>/boot/</HostDir>
      <ContainerDir>/FLASH</ContainerDir>
      <Mode>ro</Mode>
    </Volume>
  </Data>
  <Environment>
    <Variable>
      <Value>1420</Value>
      <Name>WIDTH</Name>
      <Mode/>
    </Variable>
    <Variable>
      <Value>720</Value>
      <Name>HEIGHT</Name>
      <Mode/>
    </Variable>
  </Environment>
  <Config Name="Host Port 1" Target="8080" Default="8080" Mode="tcp" Description="Container Port: 8080" Type="Port" Display="always" Required="true" Mask="false">8080</Config>
  <Config Name="Host Path 1" Target="/UNRAID" Default="" Mode="rw,slave" Description="Container Path: /UNRAID" Type="Path" Display="always" Required="true" Mask="false">/mnt/</Config>
  <Config Name="Host Path 2" Target="/nobody/.kde" Default="" Mode="rw,slave" Description="Container Path: /nobody/.kde" Type="Path" Display="always" Required="true" Mask="false">/mnt/cache/Docker/krusader</Config>
  <Config Name="Key 1" Target="WIDTH" Default="1420" Mode="" Description="Container Variable: WIDTH" Type="Variable" Display="always" Required="false" Mask="false">1420</Config>
  <Config Name="Key 2" Target="HEIGHT" Default="720" Mode="" Description="Container Variable: HEIGHT" Type="Variable" Display="always" Required="false" Mask="false">720</Config>
  <Config Name="unassigned devices" Target="/UNASSIGNED" Default="" Mode="rw,slave" Description="Container Path: /UNASSIGNED" Type="Path" Display="always" Required="false" Mask="false">/mnt/disks/</Config>
  <Config Name="Flash drive" Target="/FLASH" Default="" Mode="ro" Description="Container Path: /FLASH" Type="Path" Display="always" Required="false" Mask="false">/boot/</Config>
</Container>
