<?xml version="1.0"?>
<Container version="2">
  <Name>plexpy</Name>
  <Repository>linuxserver/plexpy</Repository>
  <Registry>https://hub.docker.com/r/linuxserver/plexpy/</Registry>
  <Network>bridge</Network>
  <Privileged>false</Privileged>
  <Support>http://lime-technology.com/forum/index.php?topic=42435.0</Support>
  <Overview>plexpy is a python based statistics manager for plex</Overview>
  <Category>MediaServer:Other Tools:</Category>
  <WebUI>http://[IP]:[PORT:8181]/</WebUI>
  <TemplateURL>https://raw.githubusercontent.com/linuxserver/docker-templates/master/linuxserver.io/plexpy.xml</TemplateURL>
  <Icon>https://raw.githubusercontent.com/linuxserver/docker-templates/master/linuxserver.io/img/plexpy-icon.png</Icon>
  <ExtraParams/>
  <DateInstalled>1501528551</DateInstalled>
  <Description>plexpy is a python based statistics manager for plex&#xD;
  &#xD;
  . [br]&#xD;
  &#xD;
  [b][u][span style='color: #E80000;']Configuration[/span][/u][/b][br]&#xD;
  [b]8181[/b] The webui for Plexpy's webui [br]&#xD;
  [b]/config[/b]Storing Configuration and the plexpy database[br]&#xD;
  [b]/logs[/b] Map to you plex logs (required for IP logging)[br]&#xD;
  </Description>
  <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort>8181</HostPort>
        <ContainerPort>8181</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <Data>
    <Volume>
      <HostDir>/mnt/disks/ADATA_SP550_2G3520038774/Plex/Library/Application Support/Plex Media Server/Logs/</HostDir>
      <ContainerDir>/logs</ContainerDir>
      <Mode>rw,slave</Mode>
    </Volume>
    <Volume>
      <HostDir>/mnt/cache/Docker/plexpy/scripts/</HostDir>
      <ContainerDir>/scripts</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
    <Volume>
      <HostDir>/mnt/cache/Docker/plexpy</HostDir>
      <ContainerDir>/config</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <Environment>
    <Variable>
      <Value>99</Value>
      <Name>PUID</Name>
      <Mode/>
    </Variable>
    <Variable>
      <Value>100</Value>
      <Name>PGID</Name>
      <Mode/>
    </Variable>
  </Environment>
  <Config Name="Host Port 1" Target="8181" Default="8181" Mode="tcp" Description="Container Port: 8181" Type="Port" Display="always" Required="true" Mask="false">8181</Config>
  <Config Name="Host Path 1" Target="/logs" Default="" Mode="rw,slave" Description="Container Path: /logs" Type="Path" Display="always" Required="true" Mask="false">/mnt/disks/ADATA_SP550_2G3520038774/Plex/Library/Application Support/Plex Media Server/Logs/</Config>
  <Config Name="Key 1" Target="PUID" Default="99" Mode="" Description="Container Variable: PUID" Type="Variable" Display="always" Required="false" Mask="false">99</Config>
  <Config Name="Key 2" Target="PGID" Default="100" Mode="" Description="Container Variable: PGID" Type="Variable" Display="always" Required="false" Mask="false">100</Config>
  <Config Name="scripts" Target="/scripts" Default="" Mode="rw" Description="Container Path: /scripts" Type="Path" Display="always" Required="false" Mask="false">/mnt/cache/Docker/plexpy/scripts/</Config>
  <Config Name="AppData Config Path" Target="/config" Default="/mnt/user/appdata/plexpy" Mode="rw" Description="Container Path: /config" Type="Path" Display="advanced-hide" Required="true" Mask="false">/mnt/cache/Docker/plexpy</Config>
</Container>
